    "scripts": {
        "run.android.timeline": "cross-var devns run android --config=nativescript.config.timeline.js --release --clean --key-store-path $KEYSTORE_PATH --key-store-password $KEYSTORE_PASSWORD --key-store-alias $KEYSTORE_ALIAS --key-store-alias-password $KEYSTORE_ALIAS_PASSWORD --env.timeline ",
        "run.android.production": "rm -fr platforms/android; cross-var devns run android --release --key-store-path $KEYSTORE_PATH --key-store-password $KEYSTORE_PASSWORD --key-store-alias $KEYSTORE_ALIAS --key-store-alias-password $KEYSTORE_ALIAS_PASSWORD --env.adhoc",
        "build.android.production": "rm -fr platforms/android; cross-var devns build android --release --clean --key-store-path $KEYSTORE_PATH --key-store-password $KEYSTORE_PASSWORD --key-store-alias $KEYSTORE_ALIAS --key-store-alias-password $KEYSTORE_ALIAS_PASSWORD --env.adhoc --env.sourceMap --env.sentry--env.uploadSentry --env.report --aab --gradleArgs=\"-PabiFilters=arm64-v8a,armeabi-v7a\"",
        "build.android.production.fdroid": "rm -fr platforms/android; cross-var devns build android --config=nativescript.config.timeline.js --release --clean --key-store-path $KEYSTORE_PATH --key-store-password $KEYSTORE_PASSWORD --key-store-alias $KEYSTORE_ALIAS --key-store-alias-password $KEYSTORE_ALIAS_PASSWORD --env.adhoc --env.report --gradleArgs=-PsplitEnabled  --gradleArgs=-PabiFilters=arm64-v8a,armeabi-v7a",
        "run.ios.production": "tns run ios --release --env.adhoc --for-device --provision 'match AppStore com.akylas.weather'",
        "peakfinder": "webpack --config peakfinder.webpack.config.js"
    },
